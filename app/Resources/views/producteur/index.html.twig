{% extends 'base.html.twig' %}

{% block body %}
    <h1>Producteurs list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Statutlegal</th>
                <th>Ninea</th>
                <th>Siegesocial</th>
                <th>Region</th>
                <th>Ville</th>
                <th>Departement</th>
                <th>Commune</th>
                <th>Arrondissement</th>
                <th>Village</th>
                <th>Numtel</th>
                <th>Numfixe</th>
                <th>Fax</th>
                <th>Email</th>
                <th>Addresse</th>
                <th>Prenomrep</th>
                <th>Nomrep</th>
                <th>Addresserep</th>
                <th>Fonctionrep</th>
                <th>Mobilerep_</th>
                <th>Fixerep</th>
                <th>Faxerep</th>
                <th>Emailrep</th>
                <th>Nommanager</th>
                <th>Prenommanager</th>
                <th>Titremanager</th>
                <th>Telmanager</th>
                <th>Emailmanager</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for producteur in producteurs %}
            <tr>
                <td><a href="{{ path('producteur_show', { 'id': producteur.id }) }}">{{ producteur.id }}</a></td>
                <td>{{ producteur.nom }}</td>
                <td>{{ producteur.prenom }}</td>
                <td>{{ producteur.statutLegal }}</td>
                <td>{{ producteur.ninea }}</td>
                <td>{{ producteur.siegeSocial }}</td>
                <td>{{ producteur.region }}</td>
                <td>{{ producteur.ville }}</td>
                <td>{{ producteur.departement }}</td>
                <td>{{ producteur.commune }}</td>
                <td>{{ producteur.arrondissement }}</td>
                <td>{{ producteur.village }}</td>
                <td>{{ producteur.numTel }}</td>
                <td>{{ producteur.numFixe }}</td>
                <td>{{ producteur.fax }}</td>
                <td>{{ producteur.email }}</td>
                <td>{{ producteur.addresse }}</td>
                <td>{{ producteur.prenomRep }}</td>
                <td>{{ producteur.nomRep }}</td>
                <td>{{ producteur.addresseRep }}</td>
                <td>{{ producteur.fonctionRep }}</td>
                <td>{{ producteur.mobileRep }}</td>
                <td>{{ producteur.fixeRep }}</td>
                <td>{{ producteur.faxeRep }}</td>
                <td>{{ producteur.emailRep }}</td>
                <td>{{ producteur.nomManager }}</td>
                <td>{{ producteur.prenomManager }}</td>
                <td>{{ producteur.titreManager }}</td>
                <td>{{ producteur.telManager }}</td>
                <td>{{ producteur.emailManager }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('producteur_show', { 'id': producteur.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('producteur_edit', { 'id': producteur.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('producteur_new') }}">Create a new producteur</a>
        </li>
    </ul>
{% endblock %}
