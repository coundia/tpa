{% extends 'base.html.twig' %}

{% block body %}
    <br/> <br/>
    <div class="row">
        <div class="col-md-4 col-md-offset-8">
            <a href="{{ path('valider_inspection', { 'id': id }) }}" class="btn btn-success">Valider</a>
           <a href="{{ path('rejeter_inspection', { 'id': id }) }}" class="btn btn-danger">Rejeter</a>
            <a href="{{ path('rapport_inspection_index') }}" class="btn btn-primary">Retour à la liste</a>
        </div>
    </div>

{% for exportation in exportation %}
    <h1 class="text-center"> Nom de l'Entreprise: {{exportation.denomEntreprise}}</h1>
{% endfor %}
    <table class="table">
        {% for exportation in exportation %}
        <tbody>

        <tr>
            <th>Id</th>
            <td>{{ exportation.id }}</td>

        </tr>

        <tr>
            <th>Date d'inspection</th>
            <td>{% if exportation.dateInspection %}{{ exportation.dateInspection|date('Y-m-d ') }}{% endif %}</td>
        </tr>
        </tbody>
        {% endfor %}
    </table>






    <div class="panel panel-primary">
        {#<div class="panel-heading">Les certificats</div>#}
        <div class="panel-body">
            <div class="row">
                {% for exportation in exportation %}

                    <div class="col-sm-6 col-md-4">
